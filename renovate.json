{
  "extends": ["config:base"],
  "automerge": false,
  "lockFileMaintenance": false,
  "rangeStrategy": "pin",
  "timezone": "America/New_York",
  "masterIssue": true,
  "prHourlyLimit": 0,
  "prConcurrentLimit": 0,
  "packageRules": [
    {
      "packageNames": ["debug", "chalk"],
      "groupName": "debug and chalk"
    },
    {
      "packagePatterns": "^@types",
      "groupName": "@types"
    },
    {
      "depTypeList": [
        "devDependencies"
      ],
      "enabled": true
    }
  ]
}
